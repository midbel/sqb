{
	"$schema": "https://biomejs.dev/schemas/1.7.0/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"formatter": {
		"enabled": true,
		"lineEnding": "lf",
		"lineWidth": 80,
		"indentStyle": "tab",
		"indentWidth": 2
	},
	"json": {
		"formatter": {
			"trailingCommas": "none"
		}
	}
}
